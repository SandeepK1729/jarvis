{
  "name": "@sandeepk1729/jarvis",
  "version": "0.2.0",
  "description": "Jarvis is a CLI tool that lets you create custom aliases for commands and paths, so you can run complex commands using simple shortcuts.",
  "main": "./dist/index.js",
  "bin": {
    "jarvis": "./dist/index.js"
  },
  "scripts": {
    "dev": "tsc -w & ncc build src/index.ts --out dist --watch",
    "build": "tsc && ncc build src/index.ts --out dist",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SandeepK1729/jarvis.git"
  },
  "keywords": [
    "CLI",
    "jarvis",
    "alias",
    "commands"
  ],
  "author": "SandeepK1729",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SandeepK1729/jarvis/issues"
  },
  "homepage": "https://github.com/SandeepK1729/jarvis#readme",
  "devDependencies": {
    "@clack/prompts": "^0.11.0",
    "@types/node": "^24.3.0",
    "@vercel/ncc": "^0.38.3",
    "commander": "^14.0.0",
    "lowdb": "^7.0.1",
    "typescript": "^5.9.2"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "directory": "dist"
  }
}
